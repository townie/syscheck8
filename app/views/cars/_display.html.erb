<table>
<tr>
  <td>year</td>
  <td>mileage</td>
  <td>color</td>
  <td>description</td>
  <td>manufacture</td>
</tr>
<% @cars.each do |car| %>
  <tr>
    <td>
      <%= car.year %>
    </td>
    <td>
     <%= car.mileage %>
    </td>
    <td>
     <%= car.color %>
    </td>
    <td>
     <%= link_to car.description, car_path(car.id) %>
    </td>
    <td>
     <%= car.manufacture ? car.manufacture.name : '' %>
    </td>
 </tr>
 <% end %>
